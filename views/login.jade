extends layout

block content
	center
		h1= title
		form(action="/authenticate", method="post")
			div(id="field-container", class="container")
				label UserName
				input(type='text',name='username', maxlength='20') 
				br
				label Password
				input(type='password', name='password', maxlength='20')
				br
				input(type='submit', value='login')